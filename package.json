{
  "name": "dullard",
  "version": "1.0.0-beta1",
  "description": "Let the computers do the boring stuff.",
  "usage": "dullard -d <dir>,...,<dirN> <step1> ... <stepN>",
  "bin": "./bin/cli.js",
  "main": "./lib/build.js",
  "scripts": {
    "test": "npm run-script jscs && npm run-script jshint && npm run-script istanbul",
    "jshint": "jshint .",
    "jscs": "jscs -c ./.jscsrc lib/",
    "istanbul": "istanbul test --print both ./node_modules/mocha/bin/_mocha",
    "mocha": "mocha"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tivac/dullard.git"
  },
  "author": "Pat Cavit <pcavit@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tivac/dullard/issues"
  },
  "devDependencies": {
    "mocha": "~1.18.2",
    "istanbul": "~0.2.0",
    "jscs": "~1.3.0",
    "jshint": "~2.5.0"
  },
  "dependencies": {
    "optimist": "~0.6.0",
    "async": "~0.7.0",
    "lodash": "~2.4.1",
    "uppity": "~0.1.1",
    "humanize-duration": "~0.8.0",
    "glob": "~3.2.7",
    "npmlog": "~0.0.6",
    "strip-json-comments": "~0.1.1"
  },
  "engines": {
    "node": ">=0.10"
  }
}
